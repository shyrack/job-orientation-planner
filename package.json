{
  "private": true,
  "main": "main/index.js",
  "name": "job-orientation-planner",
  "version": "0.0.1",
  "productName": "Job Orientation Planner",
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next",
    "dev": "npm run build-electron && electron .",
    "build-renderer": "next build renderer",
    "build-electron": "tsc -p electron-src",
    "build": "npm run build-renderer && npm run build-electron",
    "pack-app": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "type-check": "tsc -p ./renderer/tsconfig.json && tsc -p ./electron-src/tsconfig.json",
    "make": "npm run clean && npm run build && npx electron-packager . job-orientation-planner --all",
    "debugging-backtracking": "ts-node ./renderer/model/data/main.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@mui/x-data-grid": "^7.0.0",
    "@react-spring/web": "^9.7.3",
    "docx": "^8.5.0",
    "electron-is-dev": "^1.2.0",
    "electron-next": "^3.1.5",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@electron/packager": "^18.1.3",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@types/sqlite3": "^3.1.11",
    "electron": "^29.1.5",
    "electron-builder": "^24.13.3",
    "next": "^14.1.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ]
  }
}
